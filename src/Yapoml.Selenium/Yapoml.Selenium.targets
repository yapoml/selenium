<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<ItemGroup>
		<AdditionalFiles Include="**\*.po.yaml" />
		<None Remove="**\*.po.yaml" />
    <AdditionalFiles Include="**\*.po.yml" />
    <None Remove="**\*.po.yml" />
		<AdditionalFiles Include="**\*.pc.yaml" />
		<None Remove="**\*.pc.yaml" />
    <AdditionalFiles Include="**\*.pc.yml" />
    <None Remove="**\*.pc.yml" />
	</ItemGroup>

	<ItemGroup Condition="'$(ImplicitUsings)' == 'true' or '$(ImplicitUsings)' == 'enable'">
		<Using Include="Yapoml.Selenium" />
		<Using Include="$(RootNamespace)" />
	</ItemGroup>

</Project>